{
  "name": "dgraphql",
  "version": "0.1.0",
  "description": "Generate graphql schema for dgraph.",
  "main": "index.js",
  "repository": "http://github.com/dpeek/dgraphql.git",
  "author": "David Peek",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "babel src --optional runtime --ignore __tests__ --out-dir lib/",
    "start": "babel-node example",
    "dgraph": "docker rm dgraphql; docker run -it -p 8080:8080 --name dgraphql dgraph/dgraph dgraph --bindall=true"
  },
  "dependencies": {
    "graphql": "^0.10.1",
    "graphql-relay": "^0.5.1",
    "isomorphic-fetch": "^2.2.1",
    "pluralize": "^5.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^2.13.1",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "flow-bin": "^0.46.0",
    "jest": "^20.0.4"
  }
}
